\]

\end_inset


\begin_inset Formula 
\[
\text{var}(x_{t})=\sigma^{2}\frac{1}{1-\rho^{2}}
\]

\end_inset


\begin_inset Formula 
\[
\gamma(k)=\rho^{|k|}\sigma^{2}\frac{1}{1-\sigma^{2}}
\]

\end_inset

suggesting it is covariance stationary.
 Define
\begin_inset Formula 
\[
y=\begin{bmatrix}x_{1}\\
\vdots\\
x_{T}
\end{bmatrix}
\]

\end_inset


\begin_inset Formula 
\[
X=\begin{bmatrix}1 & x_{0}\\
\vdots & \vdots\\
1 & x_{T-1}
\end{bmatrix}
\]

\end_inset

Then
\begin_inset Formula 
\[
\hat{\rho}=\rho+(X'X)^{-1}\sum_{t=1}^{T}x_{t-1}e_{t}
\]

\end_inset

and
\begin_inset Formula 
\begin{align*}
\mathbb{E}\hat{\rho} & =\rho+\mathbb{E}[\mathbb{E}[(X'X)^{-1}\sum_{t=1}^{T}x_{t-1}e_{t}|X]]\\
 & =\rho+\mathbb{E}[(X'X)^{-1}\sum_{t=1}^{T}\mathbb{E}[x_{t-1}e_{t}|X]]
\end{align*}

\end_inset

Note that given 
\begin_inset Formula $\sigma(X)=\sigma((x_{t})_{t\leq T-1}$
\end_inset

,
\begin_inset Formula 
\[
x_{t-1}\uparrow\Rightarrow e_{t}\downarrow
\]

\end_inset

when 
\begin_inset Formula $\rho>0$
\end_inset

.
 Therefore, we have
\begin_inset Formula 
\[
\mathbb{E}[x_{t-1}e_{t}|X]<0
\]

\end_inset

for 
\begin_inset Formula $t=1,\cdots,T-1$
\end_inset

 and
\begin_inset Formula 
\[
\mathbb{E}\hat{\rho}<\rho
\]

\end_inset

However, we have a consistency of 
\begin_inset Formula $\hat{\rho}$
\end_inset

 from
\begin_inset Formula 
\begin{align*}
\lim_{T\rightarrow\infty}\hat{\rho} & =\rho+(\frac{1}{T}X'X)^{-1}\frac{1}{T}\sum_{t=1}^{T}x_{t-1}e_{t}\\
 & \rightarrow_{p}\rho+(\mathbb{E}[X'X])^{-1}\mathbb{E}[x_{t-1}e_{t}]\\
 & =\rho
\end{align*}

\end_inset

since
\begin_inset Formula 
\begin{align*}
\mathbb{E}[X'X] & =\begin{bmatrix}1 & \mathbb{E}x_{t}\\
\mathbb{E}x_{t} & \mathbb{E}x_{t}^{2}
\end{bmatrix}\\
 & =\begin{bmatrix}1 & \theta/(1-\rho)\\
\theta(1-\rho) & \sigma^{2}(1-\rho^{2})+\theta^{2}/(1-\rho)^{2}
\end{bmatrix}\\
 & <\infty
\end{align*}

\end_inset

Next, define
\begin_inset Formula 
\[
r=\begin{bmatrix}r_{1}\\
\vdots\\
r_{T}
\end{bmatrix}
\]

\end_inset

Then
\begin_inset Formula 
\[
\hat{\beta}=\beta+(X'X)^{-1}\sum_{t=1}^{T}x_{t-1}u_{t}
\]

\end_inset

and
\begin_inset Formula 
